package DataStructure.array;

public class ReverseAnArray {

	public static void reverse(int[] arr) {
		
		int[] newArr= new int[arr.length];
		int len = arr.length;
		for(int i=0;i<len;i++){
			newArr[i] = arr[len-1-i];
		}
	}

	public static void main(String[] args) {

		int[] arr = { 1, 2, 3, 4, 5, 6 };

		reverse(arr);
	}

}
