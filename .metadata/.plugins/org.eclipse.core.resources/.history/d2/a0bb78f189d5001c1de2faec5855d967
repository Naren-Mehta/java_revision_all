package functionalInterfaces.suppliers;

import java.util.function.Supplier;

public class GenerateRandomNumberOTP {

	public static void main(String[] args) {

		
		Supplier<Integer> s1 = ()->{
			
			
			for(int i=0;i<6;i++) {
				
			}
			
			int otp = (int)(Math.random()*1000000);
			return otp;
		};
		
		System.out.println(s1.get());
		
		
		
		
		
	}

}
