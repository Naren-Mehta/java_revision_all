package DataStructure.array;

public class ArrayPassByReference {

	public static void test(int [] newArr) {
		newArr[2]=200;
	}
	public static void main(String[] args) {
		int arr[] = {1,2,3,4,5,6};
		
		for(int i: arr) {
			System.out.println(i);
		}
		
		for(int i: arr) {
			System.out.println(i);
		}
		
		test(arr);
		
		System.out.println("=======after===========");
		for(int i: arr) {
			System.out.println(i);
		}
		
		
	}

}
