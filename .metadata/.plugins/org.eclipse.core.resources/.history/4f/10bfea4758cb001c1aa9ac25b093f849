package threads;

public class PrintEvenOddViaThreads {

	public static void main(String[] args) {

		Thread t1 = new Thread(new Runnable() {

			@Override
			public void run() {
				mt.printEvenNumbers();
			}
		});

		Thread t2 = new Thread(new Runnable() {

			@Override
			public void run() {
				mt.printOddNumbers();
			}
		});

	}

}
