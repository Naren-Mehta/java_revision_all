package threads;

class ThreadDemo extends Thread{
	
	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("Child thread  " + i + " " + Thread.currentThread().getName());
		}
	}
}
public class ThreadWIthPriority {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
