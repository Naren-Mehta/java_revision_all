package threads;

class ThreadA extends Thread{
	int total =0;
	public void run() {
		synchronized (this) {
				System.out.println("===child thread starts===============");
				for(int i=0;i<=100;i++) {
					total = total + i;
				}
		}
	}
}
public class WaitNotifyThreadExample {

	public static void main(String[] args) {

	}

}
